<odoo>
    <!-- Form View of College Semester-->
    <record id ="view_college_semester_form" model="ir.ui.view">
        <field name="name">view.college.semester.form</field>
        <field name="model">college.semester</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="code"/>         <!--placeholder="Enter your contact number"-->

                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- ================================================================== -->
    <!--Tree view College Semester-->
    <record id ="view_college_semester_tree" model="ir.ui.view">
        <field name="name">view.college.semester.tree</field>
        <field name="model">college.semester</field>
        <field name="arch" type="xml">
        <tree>
            <field name="name"/>
            <field name="code"/>
        </tree>
        </field>
    </record>


